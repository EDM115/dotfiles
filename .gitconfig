[alias]
  fpush = push --force-with-lease
[branch]
  sort = -committerdate
[color]
  ui = auto
[color "branch"]
  current = cyan bold reverse
  local = white
  plain =
  remote = cyan
[color "diff"]
  commit =
  func = cyan
  plain =
  whitespace = magenta reverse
  meta = white
  frag = cyan bold reverse
  old = red
  new = green
[color "grep"]
  context =
  filename =
  function =
  linenumber = white
  match =
  selected =
  separator =
[color "interactive"]
  error =
  header =
  help =
  prompt =
[color "status"]
  added = green
  changed = yellow
  header =
  localBranch =
  nobranch =
  remoteBranch = cyan bold
  unmerged = magenta bold reverse
  untracked = red
  updated = green bold
[column]
  ui = always
[commit]
  cleanup = whitespace
  gpgsign = true
  verbose = true
[core]
  autocrlf = input
  commitGraph = true
  editor = 'D:/EDM115/Programmes/Notepad++/notepad++.exe' -multiInst -notabbar
  eol = lf
  fsmonitor = true
  ignorecase = false
  longpaths = true
  pager = delta
  preloadIndex = true
  progress = true
  untrackedCache = true
[delta]
  light = false
  navigate = true
  syntax-theme = Dracula
[diff]
  algorithm = histogram
  colorMoved = default
  mnemonicPrefix = true
  renames = true
  tool = difftastic
[difftool]
  prompt = false
[difftool "difftastic"]
  cmd = difft "$LOCAL" "$REMOTE"
[feature]
  manyFiles = true
[fetch]
  all = true
  fsckObjects = true
  prune = true
  pruneTags = true
  writeCommitGraph = true
[filter "lfs"]
  clean = git-lfs clean -- %f
  process = git-lfs filter-process
  required = true
  smudge = git-lfs smudge -- %f
[gc]
  writeCommitGraph = true
[gitbutler]
  aiModelProvider = openai
[gpg]
  program = D:\\EDM115\\Programmes\\GnuPG\\bin\\gpg.exe
[help]
  autocorrect = 10
[http]
  postBuffer = 2147483648
[include]
  path = ~/.gitconfig.local
[index]
  version = 4
[init]
  defaultBranch = master
[interactive]
  diffFilter = delta --color-only
[merge]
  conflictStyle = zdiff3
[pack]
  usePathWalk = true
[pager]
  difftool = true
[pull]
  rebase = false
[push]
  autoSetupRemote = true
  default = simple
  followTags = true
  verbose = true
[rebase]
  updateRefs = true
[receive]
  fsckObjects = true
[rerere]
  autoUpdate = true
  enabled = true
[tag]
  forceSignAnnotated = true
  sort = version:refname
[transfer]
  fsckObjects = true
[url "https://github.com/dracula/"]
  insteadOf = dracula://
[user]
  name = EDM115
